<view class="page">
  <view class="banner">
    <image src="http://office.jiehai.gs/program/cases_banner.png" mode="widthFix" style="width:100%;" />
  </view>
  <view class="cases">
    <view class="brand-header">
      <view class="weui-article__h1">案例</view>
      <view class="weui-article__h3">- Case Show -</view>
    </view>

    <view class="case_content">
      <view class="case"  wx:for="{{[1, 2, 3,4,5,6,7,8]}}" wx:key="unique" data-id="{{item}}" bindtap='gotoCase'>
<image src="http://office.jiehai.gs/program/case_0{{item}}.png" mode="widthFix" style="width:100%;"></image>
      </view>
     
    </view>
    <view>
      <view class="weui-article__h3" style="color:rgb(200,200,200);top:20px;">- 姑且只显示这些 -</view>
    </view>

  </view>
</view>

<contact-button type="default-dark" size="20" session-from="weapp" class="img-plus-style zindex100 yc">
</contact-button>
<image src="/images/kefu.png" class="img-plus-style"></image>  
<image src="/images/call.png" class="img-plus-style1" bindtap="calling"></image>